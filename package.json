{
  "name": "codenames",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "moment": "^2.19.1",
    "nanoid": "^0.2.2",
    "react": "^16.0.0",
    "react-classset": "0.0.2",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.1.2",
    "react-transition-group": "^2.2.0",
    "sanity": "^0.2.2",
    "semantic-ui-css": "^2.2.11",
    "semantic-ui-react": "^0.75.1",
    "shuffle-array": "^1.0.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "titlecase": "^1.1.2",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon --verbose --watch dist/server/ --watch dist/shared/ dist/server/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "jest --forceExit --coverage",
    "test-watch": "jest --forceExit --watch --coverage",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json --type-check -t stylish",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix --type-check -t stylish",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-client": "webpack --watch"
  },
  "main": "server/index.ts",
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/classnames": "^2.2.3",
