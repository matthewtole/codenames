    "@types/shuffle-array": "0.0.28",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.31",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.4",
    "app-root-path": "^2.0.1",
