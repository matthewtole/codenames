    "jest": "^21.2.1",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "promise": "^8.0.1",
    "puppeteer": "^0.12.0",
    "react-dev-utils": "^4.1.0",
